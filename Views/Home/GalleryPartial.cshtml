<section
  id="slider"
  class="slider-element slider-parallax swiper_wrapper min-vh-60 min-vh-md-90"
  data-loop="true"
  data-autoplay="5000"
  data-speed="650"
>
  <div class="slider-inner">
    <div class="swiper swiper-parent">
      <div class="swiper-wrapper">
        <!-- Slide 1 -->
        <div class="swiper-slide dark">
          <div class="container">
            <div class="slider-caption">
              <h2 data-animate="fadeInUp" data-delay="100" class="galleryTitle">
                ESPECIALISTAS EN ACCIDENTES DE TRÁNSITO, MOTOS Y LESIONES
              </h2>
              <p data-animate="fadeInDown" data-delay="100">
                <a href='@Url.Action("accidentes-de-transito", "Faq")' class="button">
                  Más info
                </a>
              </p>
            </div>
          </div>
          <div class="swiper-slide-bg galleryImg1">
           </div>
        </div>

        <!-- Slide 2 -->
        <div class="swiper-slide dark">
          <div class="container">
            <div class="slider-caption">
              <h2 data-animate="fadeInUp" data-delay="100" class="galleryTitle">
                Accidentes laborales, reclamos <br />
                ante la SRT
              </h2>
              <p data-animate="fadeInDown" data-delay="100">
                <a href='@Url.Action("accidentes-laborales", "Faq")' class="button">
                  Más info
                </a>
              </p>
            </div>
          </div>
          <div class="swiper-slide-bg galleryImg2">
          </div>
        </div>

        <!-- Slide 3 -->
        <div class="swiper-slide dark">
          <div class="container">
            <div class="slider-caption slider-caption-left">
              <h2 data-animate="fadeInUp" data-delay="100" class="galleryTitle">
                Derecho laboral <br />
                y despidos
              </h2>
              <p data-animate="fadeInDown" data-delay="100">
                <a href='@Url.Action("derechos-del-trabajador", "Dismissals")' class="button">
                  Más info
                </a>
              </p>
            </div>
          </div>
          <div class="swiper-slide-bg galleryImg3">
          </div>
        </div>
      </div>

      <!-- Navigation -->
      <div class="slider-arrow-left"><i class="uil uil-angle-left-b"></i></div>
      <div class="slider-arrow-right"><i class="uil uil-angle-right-b"></i></div>
      <div class="swiper-pagination"></div>
    </div>
  </div>
</section>
