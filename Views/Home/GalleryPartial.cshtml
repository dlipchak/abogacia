<section
  id="slider"
  class="slider-element slider-parallax swiper_wrapper min-vh-75 min-vh-md-80"
  data-loop="true"
  data-autoplay="5000"
  data-speed="650"
>
  <div class="slider-inner">
    <div class="swiper-container swiper-parent">
      <div class="swiper-wrapper">
        <div class="swiper-slide dark">
          <div class="container">
            <div class="slider-caption">
              <h2 data-animate="fadeInUp" data-delay="100" class="galleryTitle">
                ESPECIALISTAS EN ACCIDENTES DE TRÁNSITO, MOTOS Y LESIONES
              </h2>
              <p data-animate="fadeInDown" data-delay="100">
                <a
                  href='@Url.Action("accidentes-de-transito", "Faq")'
                  class="button"
                >
                  Más info</a
                >
              </p>
            </div>
          </div>
          <div class="swiper-slide-bg galleryImg1"></div>
        </div>

        <div class="swiper-slide dark">
          <div class="container">
            <div class="slider-caption">
              <h2 data-animate="fadeInUp" data-delay="100" class="galleryTitle">
                Accidentes laborales, reclamos <br />
                ante la SRT
              </h2>
              <p data-animate="fadeInDown" data-delay="100">
                <a
                  href='@Url.Action("accidentes-laborales", "Faq")'
                  class="button"
                >
                  Más info</a
                >
              </p>
              <!-- <p class="d-none d-sm-block" data-animate="fadeInUp" data-delay="200">Especialistas en reclamos por accidentes laborales en la SRT</p> -->
            </div>
          </div>
          <div class="swiper-slide-bg galleryImg2"></div>
        </div>
        <div class="swiper-slide dark">
          <div class="container">
            <div class="slider-caption slider-caption-left">
              <h2 data-animate="fadeInUp" data-delay="100" class="galleryTitle">
                Derecho laboral <br />
                y despidos
              </h2>
              <p data-animate="fadeInDown" data-delay="100">
                <a
                  href='@Url.Action("derechos-del-trabajador", "Dismissals")'
                  class="button"
                  >Más info</a
                >
              </p>
              <!-- <p
                class="d-none d-sm-block"
                data-animate="fadeInUp"
                data-delay="200"
              >
                Create just what you need for your Perfect Website. Choose from
                a wide range of Elements &amp; simply put them on our Canvas.
              </p> -->
            </div>
          </div>
          <div class="swiper-slide-bg galleryImg3"></div>
        </div>
      </div>
      <div class="slider-arrow-left"><i class="icon-angle-left"></i></div>
      <div class="slider-arrow-right"><i class="icon-angle-right"></i></div>
      <!-- <div class="slide-number">
        <div class="slide-number-current"></div>
        <span>/</span>
        <div class="slide-number-total"></div>
      </div> -->
      <div class="swiper-pagination swiper-pagination-bullets"></div>
    </div>
  </div>
</section>
