<!-- prettier-ignore -->
@using System.Web.Optimization
@using System.Web
@inject Microsoft.AspNetCore.Hosting.IHostingEnvironment env
@using Microsoft.AspNetCore.Hosting
@using AbogaciaCore.Helpers;

<!DOCTYPE html>
<html>
  <head>
    <!-- set the encoding of your site -->
    <meta charset="utf-8" />
    <!-- set the viewport width and initial-scale on mobile devices -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- include the site stylesheet -->
    @*@Styles.Render("~/css/font-awesome.min.css")
    @Styles.Render("~/css/fancybox.css") @Styles.Render("~/css/bootstrap.css")
    @Styles.Render("~/css/bootstrap-dialog.min.css")
    @Styles.Render("~/css/all.css")
    @Styles.Render("~/css/jQueryUI/jquery-ui-1.10.4.custom.min.css")*@

    <link rel="icon" class="favicon" href="/images/logo.ico" />

    <link
      href="https://fonts.googleapis.com/css?family=Lato:300,400,400i,700|Poppins:300,400,500,600,700|PT+Serif:400,400i&display=swap"
      rel="stylesheet"
      type="text/css"
    />
    @if (@env.IsDevelopment()) {
    <link rel="stylesheet" href="~/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/style.css" type="text/css" />
    <link rel="stylesheet" href="~/css/dark.css" type="text/css" />

    <!-- <link rel="stylesheet" href="~/style-vars.css" type="text/css" /> -->

    <link rel="stylesheet" href="~/css/swiper.css" type="text/css" />
    <!-- <link rel="stylesheet" href="~/css/dark-vars.css" type="text/css" /> -->
    <link rel="stylesheet" href="~/css/font-icons.css" type="text/css" />
    <link rel="stylesheet" href="~/css/animate.css" type="text/css" />
    <link rel="stylesheet" href="~/css/magnific-popup.css" type="text/css" />
    <!-- 
    <link rel="stylesheet" href="~/css/responsive-vars.css" type="text/css" /> -->
    <link rel="stylesheet" href="~/abogacia.css" type="text/css" />

    <!-- <link rel="stylesheet" href="~/css/site.min.css" /> -->
    } else {
    <link rel="stylesheet" href="~/css/site.min.css" />
    } @RenderSection("Styles", required: false)

    <a
      href="https://api.whatsapp.com/send?phone=541161980179"
      class="icon-whatsapp contact-us-whatsapp"
      target="_blank"
    ></a>
  </head>
  <body
    class="stretched page-transition"
    data-loader="2"
    data-loader-timeout="20"
  >
    <div id="wrapper" class="clearfix">
      @Html.Partial("Header") @RenderBody() @Html.Partial("Footer")
    </div>

    <div id="gotoTop" class="icon-angle-up"></div>

    @if (@env.IsDevelopment()) {
    <script src="~/js/jquery.js"></script>
    <script src="~/js/functions.js"></script>
    <script src="~/js/utils.js"></script>
    <script src="~/js/components/moment.js"></script>
    <script src="~/js/calculators/calculatorWorkDismissal.js"></script>
    <script src="~/js/calculators/calculatorTrafficAccident.js"></script>
    <script src="~/js/calculators/calculatorWorkAccident.js"></script>
    <script>
      $("body").bind("copy paste cut drag drop", function (e) {
        e.preventDefault();
      });
    </script>

    <!-- <script src="~/js/plugins.animations.js"></script>
        <script src="~/js/plugins.bootstrap.js"></script>
        <script src="~/js/plugins.easing.js"></script>
        <script src="~/js/plugins.swiper.js"></script>
        <script src="~/js/plugins.flexslider.js"></script> -->
    <!-- <script src="~/js/googleMaps.js"></script> -->
    } else {
    <script src="~/js/site.min.js"></script>
    } @RenderSection("Scripts", required: false)
  </body>
</html>
